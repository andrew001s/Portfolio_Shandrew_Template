<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Shandrew Portafolio</title>
    <link rel="icon" href="~/recursos/Sin título-9.png" type="image/x-icon">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Fredoka:wght@400;500;600;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/css/fuentes.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/navbar.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ShandrewPage.styles.css" asp-append-version="true" />
</head>
<body>
    <div>
        <header>
            <nav class="navbar navbar-expand-sm navbar-toggleable-sm ">
                <div class="container-fluid">

                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                            aria-expanded="false" aria-label="Toggle navigation">
                        <span><i class="bi bi-list"></i></span>
                    </button>
                    <div class="navbar-collapse collapse d-sm-inline-flex ">
                        <ul class="navbar-nav  d-flex justify-content-evenly flex-grow-1 ">
                            <li class="nav-item m-2 ">
                                <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                            </li>
                            <li class="nav-item m-2">

                                <a class="nav-link" asp-area="" asp-controller="Portafolio" asp-action="Index">Portfolio</a>
                            </li>
                            <li class="nav-item m-2">

                                <a class="nav-link" asp-area="" asp-controller="Home" asp-action="About_Me">About Me</a>
                            </li>
                            <li class="nav-item m-2">
                                <a class="nav-link" asp-area="" asp-controller="Home" asp-action="ContactMe">Contact Me</a>
                            </li>
                            <li class="nav-item m-2">
                                <a class="nav-link" asp-area="" asp-controller="Services" asp-action="Index">Services</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </header>

        @RenderBody()



        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        @await RenderSectionAsync("Scripts", required: false)
        <footer>

            @{
                <div class=" d-flex flex-row justify-content-center ">
                    <a href="https://www.facebook.com/Shandrewvt" target="_blank">
                        <i class="bi bi-facebook"></i>
                    </a>
                    <a href="https://www.twitch.tv/elshandrew" target="_blank">
                        <i class="bi bi-twitch"></i>
                    </a>
                    <a href="https://www.tiktok.com/@@elshandrew" target="_blank">
                        <i class="bi bi-tiktok"></i>
                    </a>
                    <a href="https://www.instagram.com/elshandrew/">
                        <i class="bi bi-instagram"></i>
                    </a>
                    <a href="https://twitter.com/elshandrew" target="_blank">
                        <i class="bi bi-twitter"></i>
                    </a>
                    <a href="https://www.youtube.com/channel/UCjxyFoqzFm45CJmOB6ANqiA"
                       target="_blank">
                        <i class="bi bi-youtube"></i>
                    </a>
                    <a href="https://discord.com/invite/KtCBAfneRy" target="_blank">
                        <i class="bi bi-discord"></i>
                    </a>
                    <a href="https://www.linkedin.com/in/andr%C3%A9s-rom%C3%A1n/" target="_blank">
                        <i class="bi bi-linkedin"></i>
                    </a>
                    <a href="https://github.com/andrew001s" target="_blank">
                        <i class="bi bi-github"></i></a>
                </div>
                <div class="container text-white">


                    &copy; Shandrew - @DateTime.Now.Year
                </div>
            }
        </footer>
</body>
</html>

